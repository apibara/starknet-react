{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"]
    },
    "starknet-react-next#build": {
      "dependsOn": ["@starknet-react/core#build"]
    },
    "website#build": {
      "dependsOn": ["@starknet-react/core#build"]
    },
    "website#dev": {
      "dependsOn": ["@starknet-react/core#build"]
    },
    "test": {
      "dependsOn": ["@starknet-react/core#build"],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "lint:fix": {
      "outputs": []
    }
  }
}
