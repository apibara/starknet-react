{
  "name": "@starknet-start/providers",
  "version": "0.0.0",
  "license": "MIT",
  "repository": "starknet-start/starknet-start",
  "homepage": "https://www.starknet-react.com",
  "keywords": [
    "starknet",
    "ethereum",
    "l2"
  ],
  "type": "module",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./paymaster": {
      "types": "./dist/paymaster/index.d.ts",
      "import": "./dist/paymaster/index.js",
      "default": "./dist/paymaster/index.js"
    }
  },
  "scripts": {
    "build": "tsdown",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "test": "vitest",
    "test:ci": "vitest run",
    "lint": "biome check src",
    "lint:fix": "pnpm lint --write",
    "format:check": "biome format .",
    "format": "biome format . --write"
  },
  "dependencies": {
    "@starknet-start/chains": "workspace:*",
    "starknet": "^8.5.5"
  },
  "devDependencies": {
    "@starknet-start/typescript-config": "workspace:*",
    "rimraf": "^4.4.1",
    "tsdown": "^0.15.6",
    "vitest": "^1.6.1"
  }
}
