    "get-starknet-core": "^3.2.0",
    "jsdom": "^22.1.0",
    "moq.ts": "^9.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^4.4.1",
    "starknet": "^5.19.5",
    "tsup": "^7.2.0",
    "typedoc": "^0.24.8",
