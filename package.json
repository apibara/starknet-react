{
  "name": "starknet-react",
  "private": true,
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test": "turbo run test --parallel",
    "test:ci": "turbo run test:ci --parallel",
    "lint": "turbo run lint --parallel",
    "lint:fix": "turbo run lint:fix --parallel",
    "format": "turbo run format --parallel",
