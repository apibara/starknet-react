    "clean": "turbo run clean --parallel",
    "release": "pnpm build && pnpm changeset publish"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@types/react": "^18.2.22",
    "turbo": "^2.0.3",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=8"
  },
  "packageManager": "pnpm@8.15.3"
}
